{
  "compilerOptions": {
    "target": "ES2020",
    "jsx": "react",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "$spicetify-packages/*": ["spicetify-packages/*"]
    }
  },
  "include": [
    "./src/**/*.*"
  ]
}